    ],
    "presets": [
      "@babel/preset-env",
      "react"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "react": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux-thunk": "^2.2.0"
